# EmailJS Configuration Example
# Copy this file to .env and replace with your actual EmailJS credentials

# Service ID from EmailJS dashboard
VITE_EMAILJS_SERVICE_ID=your_service_id_here

# Template ID from EmailJS dashboard
VITE_EMAILJS_TEMPLATE_ID=your_template_id_here

# Public key from EmailJS dashboard
VITE_EMAILJS_PUBLIC_KEY=your_public_key_here

# Optional: Private key for enhanced security
# VITE_EMAILJS_PRIVATE_KEY=your_private_key_here

# Development settings
VITE_EMAIL_DEBUG=true
VITE_EMAIL_TIMEOUT=10000